function init(self)
	msg.post(".", "acquire_input_focus")
end

function on_input(self, action_id, action)
	if action.screen_x then
		if gui.pick_node(gui.get_node("next_btn"), action.screen_x, action.screen_y) and action.pressed and action_id == hash("touch") then
			msg.post("load:/proxies#load", "next", {})
		end
	end
end
